{
  "name": "football-stats-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rimraf .next && rimraf node_modules && rimraf package-lock.json",
    "generate:main": "npx prisma generate --schema=prisma/schema.prisma",
    "generate:edge": "cd supabase/functions/update-recent-performance && npx prisma generate",
    "build:mobile": "CAPACITOR_BUILD=true npm run build && npx cap sync",
    "ios:dev": "npx cap run ios --livereload --external",
    "ios:build": "npm run build:mobile && npx cap open ios",
    "android:dev": "npx cap run android --livereload --external",
    "android:build": "npm run build:mobile && npx cap open android"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/app": "^7.1.0",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/status-bar": "^7.0.3",
    "@fontsource/inter": "^5.2.5",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@prisma/client": "^6.6.0",
    "@prisma/extension-accelerate": "^1.3.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.6.5",
    "lucide-react": "^0.471.1",
    "prisma": "^6.6.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.0",
    "shadcn-ui": "^0.9.4",
    "sharp": "^0.34.2",
    "sweetalert2": "^11.18.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.3",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.6",
    "next": "^14.1.0",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3",
    "vite": "^6.2.2",
    "webpack": "^5.98.0"
  }
}
